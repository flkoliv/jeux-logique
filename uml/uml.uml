            <body>&#xD;
    panel = new PlusOuMoinsPanel(nbrEssais, nbrChrCode, false);&#xD;
    this.nbrChrCode = nbrChrCode;&#xD;
    this.nbrEssais = nbrEssais;&#xD;
    rechercheCode = new int[nbrChrCode][2];&#xD;
    for (int i = 0; i &lt; nbrChrCode; i++) {&#xD;
        rechercheCode[i][0] = 0;&#xD;
        rechercheCode[i][1] = 10;&#xD;
    }&#xD;
