          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_H7l3sJljEeeVh8pOL-FAVQ" name="getNewCode" specification="_H7kpkJljEeeVh8pOL-FAVQ">
            <language>JAVA</language>
            <body>&#xD;
    double c = Math.random();&#xD;
    String code = &quot;&quot;;&#xD;
    for (int i = 0; i &lt; nbrChrCode; i++) {&#xD;
        c = c * 10;&#xD;
    }&#xD;
    code = String.valueOf((int) c);&#xD;
    while (code.length() &lt; nbrChrCode) {&#xD;
        code = &quot;0&quot; + code;&#xD;
    }&#xD;
    super.logger.debug(&quot;Nouveau code généré par l'ordinateur : &quot; + code);&#xD;
    return code;&#xD;
</body>
